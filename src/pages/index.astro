---
import BaseLayout from "../layouts/BaseLayout.astro";
import type { Frontmatter } from "../types/Frontmatter";
const posts = await Astro.glob<Frontmatter>("./*.md");
---

<BaseLayout>
    <ul>
        {
            posts.map((post) => (
                <li>
                    <a href={post.url}>{post.frontmatter.title}</a>
                </li>
            ))
        }
    </ul>
</BaseLayout>
